<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cronpixel.chistesborrachos" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Chistes de Borrachos</name>
    <description>
        Los mejores chistes de borrachos
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        ProtonDigital Team
    </author>
    <content src="index.html" />
    <gap:plugin name="cordova-plugin-admob" source="npm" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="phonegap-version" value="cli-7.1.0" />
</widget>
